[Unit]
Description=uWSGI Emperor
After=syslog.target

[Service]
ExecStart=/etc/wifi-api/flask/bin/uwsgi --ini /etc/wifi-api/apiApp.ini 
# Requires systemd version 211 or newer
RuntimeDirectory=wifi-api
Restart=on-failure
KillSignal=SIGQUIT
Type=notify
StandardError=syslog
NotifyAccess=all

[Install]
WantedBy=multi-user.target
